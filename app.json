{
  "expo": {
    "name": "FITART",
    "slug": "fitart",
    "scheme": "fitart",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "platforms": [
      "ios",
      "android"
    ],
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fitart.app",
      "infoPlist": {
        "NSCameraUsageDescription": "FITART needs access to your camera for progress photos.",
        "NSPhotoLibraryUsageDescription": "FITART needs access to your photo library for profile and progress photos."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [],
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}